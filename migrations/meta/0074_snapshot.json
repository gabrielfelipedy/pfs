    "vw_balance_evolution": {
      "columns": {},
      "name": "vw_balance_evolution",
      "isExisting": false,
      "definition": "with \"expenses_by_daymonth\" as (select date(date, 'unixepoch', '-3 hours') as \"day\", CAST(SUM(value) AS INTEGER) as \"total_expense\" from \"vw_expense\" where day >= date('now', '-3 hours', 'start of month') AND day < date('now', '-3 hours' , '+1 month', 'start of month') group by day) select \"day\", CAST((select \"total_incomes\" from \"vw_general_balance\" limit 1) - SUM(\"total_expense\") OVER (ORDER BY \"day\" ASC) AS INTEGER) as \"balance\" from \"expenses_by_daymonth\" order by \"day\" asc"
