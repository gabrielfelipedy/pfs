    "vw_total_operation_by_daymonth": {
      "columns": {},
      "name": "vw_total_operation_by_daymonth",
      "isExisting": false,
      "definition": "with \"expenses_by_daymonth\" as (select date(date, 'unixepoch', '-3 hours') as \"day\", CAST(SUM(value) AS INTEGER) as \"total_expense\" from \"vw_expense\" where day >= date('now', 'start of month') AND day <= date('now', 'start of month', '+1 month') group by day), \"incomes_by_daymonth\" as (select date(date, 'unixepoch', '-3 hours') as \"day\", CAST(SUM(value) AS INTEGER) as \"total_income\" from \"vw_income\" where day >= date('now', 'start of month') AND day <= date('now', 'start of month', '+1 month') group by day) select COALESCE(expenses_by_daymonth.day, incomes_by_daymonth.day) as \"day\", IFNULL(\"total_income\", 0) as \"total_income\", IFNULL(\"total_expense\", 0) as \"total_expense\", (IFNULL(\"total_income\", 0) - IFNULL(\"total_expense\", 0)) as \"balance\" from \"expenses_by_daymonth\" full join \"incomes_by_daymonth\" on expenses_by_daymonth.day = incomes_by_daymonth.day order by day"
    }
