    "vw_total_expense_by_day": {
      "columns": {},
      "name": "vw_total_expense_by_day",
      "isExisting": false,
      "definition": "select CAST(SUM(\"value\") AS INTEGER) as \"total_value\" from \"vw_expense\" where \n    substr(\"vw_expense\".\"date\", 1, 10) >= date('now', 'start of day')\n    AND substr(\"vw_expense\".\"date\", 1, 10) <= date('now')\n  "
    },
    "vw_total_expense_by_daymonth": {
      "columns": {},
      "name": "vw_total_expense_by_daymonth",
      "isExisting": false,
      "definition": "select date(\"date\", 'unixepoch') as \"date\", CAST(SUM(\"value\") AS INTEGER) as \"total_value\" from \"vw_expense\" where \n    date(\"vw_expense\".\"date\", 'unixepoch') >= date('now', 'start of month')\n    AND date(\"vw_expense\".\"date\", 'unixepoch') <= date('now')\n   group by date(\"vw_expense\".\"date\", 'unixepoch') order by date(\"vw_expense\".\"date\", 'unixepoch')"
    },
